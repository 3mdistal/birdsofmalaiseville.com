    "@payloadcms/storage-vercel-blob": "beta",
    "@payloadcms/ui": "beta",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "lucide-react": "^0.378.0",
    "next": "^15.0.1",
    "payload": "beta",
    "payload-admin-bar": "^1.0.6",
    "prettier-plugin-css-order": "^2.1.2",
